<h2>Lista Categorias</h2>
<hr>
<a href="/admin/categorias/add">
    <button>Nova categoria</button>
</a>

{{#each categorias}}
    <div class="card mt-3">
        <div class="card-body">
            <h4>{{nome}}</h4>
            <small>Slug: {{slug}}</small>
            <small>{{date}}</small>
        </div>
    </div>
{{else}}

{{/each}}